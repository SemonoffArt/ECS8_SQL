                                        <ItemsPanelTemplate >
                                            <WrapPanel Orientation="Horizontal" >
                                            </WrapPanel>
                                        </ItemsPanelTemplate>
                                    </ItemsControl.ItemsPanel>
                                </EccFace:EccItemsControl>
                            </GroupBox>
                            <EccFace:EccContainerContentControl Grid.Row="5" Grid.ColumnSpan="2" Container="{Binding ., Source={StaticResource MasterData}}" FlsBase:Info.MimicPartTemplate="AcesysV8.FP_Template_Ref_Manuals"/>
                        </Grid>
                    </ScrollViewer>
                </TabItem>
                <TabItem Header="Diag" Visibility="{Binding Block[Intell_Type].Value, Source={StaticResource MasterData}, Converter={FlsBase:CompareValues GE, VisibleColapsed, 1}, FallbackValue=Collapsed}" >
                    <EccFace:EccContainerContentControl Container="{Binding ., Source={StaticResource MasterData}}" FlsBase:Info.MimicPartTemplate="AcesysV8.FP_Template_AdaptDiagTab"/>
                </TabItem>
                <TabItem Header="{FlsBase:LangByCaption Fls.Language.AcesysV8::Tab.Info}">
                    <EccFace:EccContainerContentControl Container="{Binding ., Source={StaticResource MasterData}}" FlsBase:Info.MimicPartTemplate="AcesysV8.FP_Template_InfoTab">
                        <EccFace:EccContainerContentControl.Variables>
                            <FlsBase:FlsCollection>
                                <FlsBase:FlsTextValue Key="Siemens" Value="FB 210" />
                                <FlsBase:FlsTextValue Key="Rockwell" Value="AsysMot2" />
                                <FlsBase:FlsTextValue Key="ABB" Value="ACESYS__BIMOTOR" />
                                <FlsBase:FlsTextValue Key="Schneider" Value="ACESYS_V80_MOTOR" />
                            </FlsBase:FlsCollection>
                        </EccFace:EccContainerContentControl.Variables>
                    </EccFace:EccContainerContentControl>
                </TabItem>
            </TabControl>
            <!--Data Triggers placeholders if BlockItems-->
            <Control Name="Sel_INCH_Man" IsEnabled="{Binding Block[Sel_INCH_Man].Value, Source={StaticResource MasterData}, FallbackValue=Fals}"      Width="0" Height="0" />
            <Control Name="MAN" IsEnabled="{Binding Block[MAN].Value, Source={StaticResource MasterData}, FallbackValue=Fals}"      Width="0" Height="0" />
            <!--
            <Control DataContext="{Binding ., Source={StaticResource MasterData}}" Name="INTL" IsEnabled="True"  Width="0" Height="0">
                <Control.Triggers>
                    <EventTrigger RoutedEvent="Loaded">
                        <EventTrigger.Actions>
                            <BeginStoryboard>
