<DataTemplate	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	            xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
				xmlns:sys="clr-namespace:System;assembly=mscorlib"
                xmlns:FlsBase="clr-namespace:Fls.Core.Hmi.UI.PA;assembly=Fls.Core.Hmi.UI"
                xmlns:EccFace="clr-namespace:Fls.Ecc.Pnt.UI.Faceplate.PA;assembly=Fls.Ecc.Pnt.UI.Faceplate">
    <!--_____________________________________________________________________________________________________________________________

	Build 001 - 01 Dec 2014 - First Release
	Build 002 - 22 Dec 2015 - Support of Simple AI in Related Tab  - SR1
	Build 003 - 31 Oct 2016 - Diag Tab added to diaplay Iusys data
	Build 004 - 25 Nov 2016 - Local Stop Lamp 0 state color changed to Red & Faceplate width increased to 550
	Build 005 - 01 Dec 2016 - Related Indication template added newly and changed in related point for indication
	Build 006 - 01 Feb 2017 - Power Monitor Groupbox in Related tab added
	Build 007 - 15 Mar 2017 - Faceplate Height increased from 400 to 500 to accomodate all configed tabs
	Build 008 - 17 Aug 2017 - SIM_MD_Pulses check box added in Device Simulation tab.
	Build 009 - 20 Feb 2018 - Schneider Routine name changed
    Build 010 - 01 Oct 2018 - Horizontal Scroll bar added
    Build 011 - 18 Dec 2018 - Interlock Horizontal and Vertical scroll bar corrected
    Build 012 - 19 Sep 2019 - Faceplate width applied from External sitespecific style file
    Build 013 - 14 Nov 2019 - Sim tab code optimized & Items control replaced with content control in config tab - Performance Improvement
    
	

_____________________________________________________________________________________________________________________________-->
    <Grid Width="{Binding Value, Source={FlsBase:Retrieve V8FP_LargeWidth, AcesysV8.SiteSpecific},FallbackValue=600}" 
          Resources="{FlsBase:RetrieveStyleFile AcesysV8.Styles}" FlsBase:ExtendOwner.ExtendObject="{x:Type EccFace:FaceplateViewModel}">
        <Grid Background="{DynamicResource {x:Static SystemColors.ControlBrushKey}}">
            <Grid.Resources>
                <EccFace:DataContainer  x:Key="MasterData" FunctionBlockType="Related Signals" LoadPointConfig="True"  LoadBlockparameters="true" LoadStatistics="True"/>
                <EccFace:ChildContainer x:Key="InterlockDataVis" FunctionBlockType="Interlock" />
                <EccFace:ChildContainer x:Key="RelatedSignalsVis" FunctionBlockType="Related Signals"/>
                <EccFace:ChildContainer x:Key="RelatedSimAnalog"      ReferencePoint="PointEntity" FunctionBlockType="Related Signals" FunctionBlockSubtype="Simple Analog Measurement"/>
                <EccFace:ChildContainer x:Key="RelatedAnalog"      ReferencePoint="PointEntity" FunctionBlockType="Related Signals" FunctionBlockSubtype="Analog Measurement"/>
                <EccFace:ChildContainer x:Key="RelatedDigital"     ReferencePoint="PointEntity" FunctionBlockType="Related Signals" FunctionBlockSubtype="Digital Alarm"/>
                <EccFace:ChildContainer x:Key="RelatedSelect"     ReferencePoint="PointEntity" FunctionBlockType="Related Signals" FunctionBlockSubtype="Select"/>
                <EccFace:ChildContainer x:Key="RelatedIndi"     ReferencePoint="PointEntity" FunctionBlockType="Related Signals" FunctionBlockSubtype="Indication"/>
                <EccFace:ChildContainer x:Key="RelatedPowerMonitor"     ReferencePoint="PointEntity" FunctionBlockType="Related Signals" FunctionBlockSubtype="Power Monitor"/>
            </Grid.Resources>
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
            </Grid.RowDefinitions>
            <Rectangle Stroke="Yellow" StrokeThickness="3" Grid.RowSpan="3" Visibility="{Binding Block[SIM_ACTIVE].Value, Source={StaticResource MasterData}, Converter={FlsBase:ObjectToVisibility True, Collapsed} ,FallbackValue=Collapsed}"/>
            <FlsBase:DynamicSymbol FileName="AcesysV8.FP_Header_Info" TagCode="{Binding Owner.TagCode, Source={StaticResource MasterData}}" Area="{Binding Owner.Area, Source={StaticResource MasterData}}"/>
            <TabControl Margin="3" Grid.Row="1" Height="500" Name="TabCtrl" TabStripPlacement="Right" Grid.IsSharedSizeScope="True" >
                <TabItem Header="{FlsBase:LangByCaption Fls.Language.AcesysV8::Tab.Control}">
                    <ScrollViewer VerticalScrollBarVisibility="Auto" Template="{DynamicResource ScrollViewerControl}">
                        <Grid >
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="{Binding Block[Type_Unimotor].Value, Source={StaticResource MasterData}, Converter={FlsBase:ObjectToBoolean true, {x:Static sys:Double.NaN}, 0, false}, FallbackValue=auto}"/>
                                <RowDefinition Height="{Binding Block[Type_Bimotor].Value, Source={StaticResource MasterData}, Converter={FlsBase:ObjectToBoolean true, {x:Static sys:Double.NaN}, 0, false}, FallbackValue=0}"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto" Name="AnalogValues"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>

                            <GroupBox Name="CmdUnimotor" Grid.Row="1" Header="{FlsBase:LangByCaption Fls.Language.AcesysV8::Grp.StartStop}">

                                <EccFace:EccItemsControl 	EccPanel="3:2"
														BlockParameters="{Binding Block, Source={StaticResource MasterData}}" 
														FlsBase:Info.MimicPartTemplate="AcesysV8.FP_Template_ButtonBlkItem" 
														ItemsSpecification="FP_CMD_Start1#PicText#Start1#DimOut_Start1#Show_PB_Start1;
																			Empty;
																			FP_CMD_Stop#PicText#Stop#DimOut_Stop#Show_PB_Stop;
																			Empty;
																			FP_CMD_QStop#PicText#Quick_Stop#DimOut_Qstop#Show_PB_QStop"/>
                            </GroupBox>
                            <GroupBox Name="CmdUnimotorInching" Visibility="Collapsed" Grid.Row="1" Header="{FlsBase:LangByCaption Fls.Language.AcesysV8::Grp.StartStop}">

                                <EccFace:EccItemsControl 	EccPanel="3:2"
														BlockParameters="{Binding Block, Source={StaticResource MasterData}}" 
														FlsBase:Info.MimicPartTemplate="AcesysV8.FP_Template_ButtonBlkItem" 
														ItemsSpecification="FP_CMD_Start1#PicTextInching#Start1#DimOut_Start1#Show_PB_Start1;
								                                            Empty;
								                                            FP_CMD_Stop#PicText#nextstep#DimOut_Stop#Show_PB_Stop;
								                                            Empty;
								                                            FP_CMD_QStop#PicText#Quick_Stop#DimOut_Qstop#Show_PB_QStop" />
                            </GroupBox>
                            <GroupBox Name="CmdBimotor" Grid.Row="2"  Header="{FlsBase:LangByCaption Fls.Language.AcesysV8::Grp.StartStop}" >
                                <EccFace:EccItemsControl 	EccPanel="3:2"
														BlockParameters="{Binding Block, Source={StaticResource MasterData}}" 
														FlsBase:Info.MimicPartTemplate="AcesysV8.FP_Template_ButtonBlkItem" 
														ItemsSpecification="FP_CMD_Start1#PicText#Start1#DimOut_Start1#Show_PB_Start1;
                                                                            #Empty;																			
																			FP_CMD_Start2#PicText#Start2#DimOut_Start2#Show_PB_Start2;
                                                                            #Empty;
                                                                            FP_CMD_Stop#PicText#Stop#DimOut_Stop#Show_PB_Stop;
                                                                            FP_CMD_QStop#PicText#Quick_Stop#DimOut_Qstop#Show_PB_QStop"/>
                            </GroupBox>
                            <GroupBox Name="CmdBimotorInching" Visibility="Collapsed" Grid.Row="2"  Header="{FlsBase:LangByCaption Fls.Language.AcesysV8::Grp.StartStop}" >
                                <EccFace:EccItemsControl 	EccPanel="3:2"
														BlockParameters="{Binding Block, Source={StaticResource MasterData}}" 
														FlsBase:Info.MimicPartTemplate="AcesysV8.FP_Template_ButtonBlkItem" 
														ItemsSpecification="FP_CMD_Start1#PicTextInching#Start1#DimOut_Start1#Show_PB_Start1;
                                                                            #Empty;																			
									                                        FP_CMD_Start2#PicTextInching#Start2#DimOut_Start2#Show_PB_Start2;
                                                                            #Empty;
                                                                            FP_CMD_Stop#PicText#Stop#DimOut_Stop#Show_PB_Stop;
                                                                            FP_CMD_QStop#PicText#Quick_Stop#DimOut_Qstop#Show_PB_QStop"/>
                            </GroupBox>
                            <GroupBox Grid.Row="3" Header="{FlsBase:LangByCaption Fls.Language.AcesysV8::Grp.Mode}">
                                <EccFace:EccItemsControl	EccPanel="4:1"
														BlockParameters="{Binding Block, Source={StaticResource MasterData}}" 
														FlsBase:Info.MimicPartTemplate="AcesysV8.FP_Template_ButtonBlkItem"
														ItemsSpecification="FP_CMD_Auto#Text##DimOut_Auto#Show_PB_Auto;
																			FP_CMD_Local#Text##DimOut_Local#Show_PB_Local;
																			FP_CMD_Manual#Text##DimOut_Manual#Show_PB_Manual;
																			FP_CMD_Test#Text##DimOut_Test#Show_PB_Test"/>
                            </GroupBox>
                            <GroupBox Grid.Row="4" Header="{FlsBase:LangByCaption Fls.Language.AcesysV8::Grp.AnalMes}">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="{Binding Block[SHOW_AI1].Value, Converter={FlsBase:ObjectToBoolean true, {x:Static sys:Double.NaN}, 0, false}, Source={StaticResource MasterData}, FallbackValue=auto}"/>
                                        <RowDefinition Height="{Binding Block[SHOW_AI2].Value, Converter={FlsBase:ObjectToBoolean true, {x:Static sys:Double.NaN}, 0, false}, Source={StaticResource MasterData}, FallbackValue=auto}"/>
                                        <RowDefinition Height="{Binding Block[SHOW_AI3].Value, Converter={FlsBase:ObjectToBoolean true, {x:Static sys:Double.NaN}, 0, false}, Source={StaticResource MasterData}, FallbackValue=auto}"/>
                                        <RowDefinition Height="{Binding Block[SHOW_SP].Value, Converter={FlsBase:ObjectToBoolean true, {x:Static sys:Double.NaN}, 0, false}, Source={StaticResource MasterData}, FallbackValue=auto}"/>
                                        <RowDefinition Height="*"/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>
                                    <EccFace:EccContentControl  FlsBase:Info.MimicPartTemplate="AcesysV8.FP_Template_BlkValDesc"
															BlockParameters="{Binding Block, Source={StaticResource MasterData}}"
															CommonData="Language#Show_"
															ItemSpecification="AO1a"/>
                                    <EccFace:EccContentControl	Grid.Row="1" FlsBase:Info.MimicPartTemplate="AcesysV8.FP_Template_BlkValDesc"
															BlockParameters="{Binding Block, Source={StaticResource MasterData}}"
															CommonData="Language#Show_"
															ItemSpecification="AO2a"/>
                                    <EccFace:EccContentControl	Grid.Row="2" FlsBase:Info.MimicPartTemplate="AcesysV8.FP_Template_BlkValDesc"
															BlockParameters="{Binding Block, Source={StaticResource MasterData}}"
															CommonData="Language#Show_"
															ItemSpecification="AO3a"/>
                                    <EccFace:EccContentControl	Grid.Row="3" FlsBase:Info.MimicPartTemplate="AcesysV8.FP_Template_BlkValDesc"
															BlockParameters="{Binding Block, Source={StaticResource MasterData}}"
															CommonData="Language#Show_"
															ItemSpecification="CV"/>

                                    <EccFace:EccContentControl  Grid.Column="1"	FlsBase:Info.MimicPartTemplate="AcesysV8.FP_Template_BlkVal"
															BlockParameters="{Binding Block, Source={StaticResource MasterData}}"
															ItemSpecification="AO1b"/>
                                    <EccFace:EccContentControl  Grid.Row="1" Grid.Column="1"	FlsBase:Info.MimicPartTemplate="AcesysV8.FP_Template_BlkVal"
															BlockParameters="{Binding Block, Source={StaticResource MasterData}}"
															ItemSpecification="AO2b"/>
                                    <EccFace:EccContentControl  Grid.Row="2" Grid.Column="1"	FlsBase:Info.MimicPartTemplate="AcesysV8.FP_Template_BlkVal"
															BlockParameters="{Binding Block, Source={StaticResource MasterData}}"
															ItemSpecification="AO3b"/>


                                </Grid>
                            </GroupBox>
                        </Grid>
                    </ScrollViewer>
                </TabItem>
                <TabItem Header="{FlsBase:LangByCaption Fls.Language.AcesysV8::Tab.Interlock}" Visibility="{Binding FunctionBlock.HasPoints, Source={StaticResource InterlockDataVis}, Converter={FlsBase:ObjectToVisibility True, Visible, Collapsed}, FallbackValue=Collapse}">
                    <Grid>
                        <Grid.Resources>
                            <EccFace:ChildContainer x:Key="InterlockData"      FunctionBlockType="Interlock" FunctionBlockSubtype="INTL"/>
                            <EccFace:ChildContainer x:Key="InterlockDataSTR"   ReferencePoint="PointEntity" FunctionBlockType="Interlock" FunctionBlockSubtype="INTL_SEQ_STR"/>
                            <EccFace:ChildContainer x:Key="InterlockDataSTR1"  ReferencePoint="PointEntity" FunctionBlockType="Interlock" FunctionBlockSubtype="INTL_SEQ_STR1"/>
                            <EccFace:ChildContainer x:Key="InterlockDataSTR2"  ReferencePoint="PointEntity" FunctionBlockType="Interlock" FunctionBlockSubtype="INTL_SEQ_STR2"/>
                            <EccFace:ChildContainer x:Key="InterlockDataSTP"   ReferencePoint="PointEntity" FunctionBlockType="Interlock" FunctionBlockSubtype="INTL_SEQ_STP"/>
                        </Grid.Resources>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" Template="{DynamicResource ScrollViewerControl}">

                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="{Binding Block[Type_Unimotor].Value, Source={StaticResource MasterData}, Converter={FlsBase:ObjectToBoolean true, {x:Static sys:Double.NaN}, 0, false}, FallbackValue=auto}"/>
                                    <RowDefinition Height="{Binding Block[Type_Bimotor].Value, Source={StaticResource MasterData}, Converter={FlsBase:ObjectToBoolean true, {x:Static sys:Double.NaN}, 0, false}, FallbackValue=auto}"/>
                                    <RowDefinition Height="{Binding Block[Type_Bimotor].Value, Source={StaticResource MasterData}, Converter={FlsBase:ObjectToBoolean true, {x:Static sys:Double.NaN}, 0, false}, FallbackValue=auto}"/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="*"/>
                                </Grid.RowDefinitions>
                                <!--Visibility="{Binding Block[Type_Bimotor].Value, Source={StaticResource MasterData}, Converter={FlsBase:ObjectToVisibility True, Collapsed}}"-->
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>

                                <EccFace:EccContentControl	FlsBase:Info.MimicPartTemplate="AcesysV8.FP_Template_LampBasic" Visibility="{Binding FunctionBlock.HasPoints, Source={StaticResource InterlockData}, Converter={FlsBase:ObjectToVisibility True, Visible, Collapsed}, FallbackValue=Collapse}"
														BlockParameters="{Binding Block, Source={StaticResource MasterData}}" 
														ItemSpecification="INTL#Acesys.On#Acesys.Alarm"/>

                                <GroupBox Grid.Column="1"	Header="{Binding Block[INTL].CombinedDescription, Source={StaticResource MasterData}, FallbackValue=Interlock}" 
														Visibility="{Binding FunctionBlock.HasPoints, Source={StaticResource InterlockData}, Converter={FlsBase:ObjectToVisibility True, Visible, Collapsed}, FallbackValue=Collapse}">
                                    <ItemsControl ItemsSource="{Binding FunctionBlock.TheList, Source={StaticResource InterlockData}}" FlsBase:Info.MimicPartTemplate="AcesysV8.FP_Template_InterlockLine" />
                                </GroupBox>
                                <EccFace:EccContentControl	Grid.Row="1" Visibility="{Binding FunctionBlock.HasPoints, Source={StaticResource InterlockDataSTR}, Converter={FlsBase:ObjectToVisibility True, Visible, Collapsed}, FallbackValue=Collapse}"
														FlsBase:Info.MimicPartTemplate="AcesysV8.FP_Template_LampBasic"
														BlockParameters="{Binding Block, Source={StaticResource MasterData}}" 
														ItemSpecification="INTL_SEQ_STR1#Acesys.On#Acesys.Alarm"/>
                                <GroupBox	Header="{Binding Block[INTL_SEQ_STR1].CombinedDescription, Source={StaticResource MasterData}, FallbackValue=StrIntl}" Grid.Column="1" Grid.Row="1"
										Visibility="{Binding FunctionBlock.HasPoints, Source={StaticResource InterlockDataSTR}, Converter={FlsBase:ObjectToVisibility True, Visible, Collapsed}, FallbackValue=Collapse}">
                                    <ItemsControl ItemsSource="{Binding FunctionBlock.TheList, Source={StaticResource InterlockDataSTR}}" FlsBase:Info.MimicPartTemplate="AcesysV8.FP_Template_InterlockLine" />
                                </GroupBox>
                                <EccFace:EccContentControl	Grid.Row="2" Visibility="{Binding FunctionBlock.HasPoints, Source={StaticResource InterlockDataSTR1}, Converter={FlsBase:ObjectToVisibility True, Visible, Collapsed}, FallbackValue=Collapse}"
														FlsBase:Info.MimicPartTemplate="AcesysV8.FP_Template_LampBasic"
														BlockParameters="{Binding Block, Source={StaticResource MasterData}}" 
														ItemSpecification="INTL_SEQ_STR1#Acesys.On#Acesys.Alarm"/>
                                <GroupBox Header="{Binding Block[INTL_SEQ_STR1].CombinedDescription, Source={StaticResource MasterData}, FallbackValue=Str1Intl}" Grid.Column="1" Grid.Row="2"
										Visibility="{Binding FunctionBlock.HasPoints, Source={StaticResource InterlockDataSTR1}, Converter={FlsBase:ObjectToVisibility True, Visible, Collapsed}, FallbackValue=Collapse}">
                                    <ItemsControl ItemsSource="{Binding FunctionBlock.TheList, Source={StaticResource InterlockDataSTR1}}" FlsBase:Info.MimicPartTemplate="AcesysV8.FP_Template_InterlockLine" />
                                </GroupBox>
                                <EccFace:EccContentControl	Grid.Row="3" Visibility="{Binding FunctionBlock.HasPoints, Source={StaticResource InterlockDataSTR2}, Converter={FlsBase:ObjectToVisibility True, Visible, Collapsed}, FallbackValue=Collapse}"
														FlsBase:Info.MimicPartTemplate="AcesysV8.FP_Template_LampBasic"
														BlockParameters="{Binding Block, Source={StaticResource MasterData}}" 
														ItemSpecification="INTL_SEQ_STR2#Acesys.On#Acesys.Alarm"/>
                                <GroupBox Header="{Binding Block[INTL_SEQ_STR2].CombinedDescription, Source={StaticResource MasterData}, FallbackValue=Str2Intl}" Grid.Column="1" Grid.Row="3"
										Visibility="{Binding FunctionBlock.HasPoints, Source={StaticResource InterlockDataSTR2}, Converter={FlsBase:ObjectToVisibility True, Visible, Collapsed}, FallbackValue=Collapse}">
                                    <ItemsControl ItemsSource="{Binding FunctionBlock.TheList, Source={StaticResource InterlockDataSTR2}}" FlsBase:Info.MimicPartTemplate="AcesysV8.FP_Template_InterlockLine" />
                                </GroupBox>
                                <EccFace:EccContentControl	Grid.Row="4" Visibility="{Binding FunctionBlock.HasPoints, Source={StaticResource InterlockDataSTP}, Converter={FlsBase:ObjectToVisibility True, Visible, Collapsed}, FallbackValue=Collapse}"
														FlsBase:Info.MimicPartTemplate="AcesysV8.FP_Template_LampBasic"
														BlockParameters="{Binding Block, Source={StaticResource MasterData}}" 
														ItemSpecification="INTL_SEQ_STP#Acesys.On#Acesys.Alarm"/>
                                <GroupBox Header="{Binding Block[INTL_SEQ_STP].CombinedDescription, Source={StaticResource MasterData}, FallbackValue=StpIntl}" Grid.Column="1" Grid.Row="4"
										Visibility="{Binding FunctionBlock.HasPoints, Source={StaticResource InterlockDataSTP}, Converter={FlsBase:ObjectToVisibility True, Visible, Collapsed}, FallbackValue=Collapse}">
                                    <ItemsControl ItemsSource="{Binding FunctionBlock.TheList, Source={StaticResource InterlockDataSTP}}" FlsBase:Info.MimicPartTemplate="AcesysV8.FP_Template_InterlockLine" />
                                </GroupBox>

                            </Grid>
                        </ScrollViewer>

                        <Grid Grid.Row="1">
                            <GroupBox Grid.ColumnSpan="2" Grid.Row="5" Header="{FlsBase:LangByCaption Fls.Language.AcesysV8::Grp.StpRes}">
                                <TextBlock TextAlignment="Center" Text="{Binding Block[StopReason].TextValue, Source={StaticResource MasterData}, FallbackValue=NoKey}" />
                            </GroupBox>
                        </Grid>
                    </Grid>
                </TabItem>
                <TabItem Header="{FlsBase:LangByCaption Fls.Language.AcesysV8::Tab.RelSig}" Visibility="{Binding FunctionBlock.HasPoints, Source={StaticResource RelatedSignalsVis}, Converter={FlsBase:ObjectToVisibility True, Visible, Collapsed}, FallbackValue=Collapse}">
                    <ScrollViewer VerticalScrollBarVisibility="Auto" Template="{DynamicResource ScrollViewerControl}">
                        <Grid>
                            <Grid.Resources>

                            </Grid.Resources>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <GroupBox Header="{FlsBase:LangByCaption Fls.Language.AcesysV8::Grp.RelPowerMon}" Grid.Row="0" Visibility="{Binding FunctionBlock.HasPoints, Source={StaticResource RelatedPowerMonitor}, Converter={FlsBase:ObjectToVisibility True, Visible, Collapsed}, FallbackValue=Collapse}">
                                <ItemsControl ItemsSource="{Binding FunctionBlock.TheList, Source={StaticResource RelatedPowerMonitor}}" FlsBase:Info.DynamicSymbolTemplate="AcesysV8.FP_RelatedValues" />
                            </GroupBox>
                            <GroupBox Header="{FlsBase:LangByCaption Fls.Language.AcesysV8::Grp.RelSigSA}" Grid.Row="1" Visibility="{Binding FunctionBlock.HasPoints, Source={StaticResource RelatedSimAnalog}, Converter={FlsBase:ObjectToVisibility True, Visible, Collapsed}, FallbackValue=Collapse}">
                                <ItemsControl ItemsSource="{Binding FunctionBlock.TheList, Source={StaticResource RelatedSimAnalog}}" FlsBase:Info.DynamicSymbolTemplate="AcesysV8.FP_RelatedValuesNoStatus" />
                            </GroupBox>
                            <GroupBox Header="{FlsBase:LangByCaption Fls.Language.AcesysV8::Grp.RelSigA}" Grid.Row="2" Visibility="{Binding FunctionBlock.HasPoints, Source={StaticResource RelatedAnalog}, Converter={FlsBase:ObjectToVisibility True, Visible, Collapsed}, FallbackValue=Collapse}">
                                <ItemsControl ItemsSource="{Binding FunctionBlock.TheList, Source={StaticResource RelatedAnalog}}" FlsBase:Info.DynamicSymbolTemplate="AcesysV8.FP_RelatedValues" />
                            </GroupBox>
                            <GroupBox Header="{FlsBase:LangByCaption Fls.Language.AcesysV8::Grp.RelSigB}" Grid.Row="3" Visibility="{Binding FunctionBlock.HasPoints, Source={StaticResource RelatedDigital}, Converter={FlsBase:ObjectToVisibility True, Visible, Collapsed}, FallbackValue=Collapse}">
                                <ItemsControl ItemsSource="{Binding FunctionBlock.TheList, Source={StaticResource RelatedDigital}}" FlsBase:Info.DynamicSymbolTemplate="AcesysV8.FP_RelatedValues" />
                            </GroupBox>
                            <GroupBox Header="{FlsBase:LangByCaption Fls.Language.AcesysV8::Grp.RelSigSel}" Grid.Row="4" Visibility="{Binding FunctionBlock.HasPoints, Source={StaticResource RelatedSelect}, Converter={FlsBase:ObjectToVisibility True, Visible, Collapsed}, FallbackValue=Collapse}">
                                <ItemsControl ItemsSource="{Binding FunctionBlock.TheList, Source={StaticResource RelatedSelect}}" FlsBase:Info.DynamicSymbolTemplate="AcesysV8.FP_RelatedSelections" />
                            </GroupBox>
                            <GroupBox Header="{FlsBase:LangByCaption Fls.Language.AcesysV8::Grp.RelSigInd}" Grid.Row="5" Visibility="{Binding FunctionBlock.HasPoints, Source={StaticResource RelatedIndi}, Converter={FlsBase:ObjectToVisibility True, Visible, Collapsed}, FallbackValue=Collapse}">
                                <ItemsControl ItemsSource="{Binding FunctionBlock.TheList, Source={StaticResource RelatedIndi}}" FlsBase:Info.DynamicSymbolTemplate="AcesysV8.FP_RelatedIndications" />
                            </GroupBox>
                        </Grid>
                    </ScrollViewer>
                </TabItem>
                <TabItem Header="{FlsBase:LangByCaption Fls.Language.AcesysV8::Tab.Input}" >
                    <ScrollViewer VerticalScrollBarVisibility="Auto" Template="{DynamicResource ScrollViewerControl}">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="0.5*" />
                                <ColumnDefinition Width="0.5*" />
                            </Grid.ColumnDefinitions>
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                </Grid.RowDefinitions>
                                <GroupBox Header="{FlsBase:LangByCaption Fls.Language.AcesysV8::Grp.Input}">
                                    <EccFace:EccItemsControl	FlsBase:Info.MimicPartTemplate="AcesysV8.FP_Template_LampLine"
																BlockParameters="{Binding Block, Source={StaticResource MasterData}}" 
																CommonData="Show_#ACESYS_"
																ItemsSpecification="RDY#RDY#Acesys.Ok#Acesys.Alarm;
																					MCC#MCC#Acesys.Ok#Acesys.Alarm;
																					OVL#OVL#Acesys.Ok#Acesys.Alarm;
																					RET1#RET1#Acesys.On#Acesys.Stop;
																					RET2#RET2#Acesys.On#Acesys.Stop;
																					LSTR1#LSTR1#Acesys.On#Acesys.Off;
																					LSTR2#LSTR2#Acesys.On#Acesys.Off;
																					LSTP#LSTP#Acesys.On#Acesys.Alarm;
																					REM#REM#Acesys.Ok#Acesys.Alarm;
																					SSW#SSW#Acesys.Ok#Acesys.Alarm;
																					MD#MD#Acesys.On#Acesys.Off;
																					SFS#SFS#Acesys.On#Acesys.Off;
																					SLS#SLS#Acesys.On#Acesys.Off;
																					ES#ES#Acesys.Ok#Acesys.Alarm"/>
                                </GroupBox>
                                <GroupBox Grid.Row="1" Header="{FlsBase:LangByCaption Fls.Language.AcesysV8::Grp.Misc}">
                                    <EccFace:EccItemsControl	FlsBase:Info.MimicPartTemplate="AcesysV8.FP_Template_LampLine"
																BlockParameters="{Binding Block, Source={StaticResource MasterData}}" 
																CommonData="Show_#ACESYS_"
																ItemsSpecification="SEL#SEL#Acesys.On#Acesys.Off;
																					ASTR#ASTR#Acesys.On#Acesys.Off"/>
                                </GroupBox>
                            </Grid>
                            <Grid Grid.Column="1">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" x:Name="Ext_Input"/>
                                    <RowDefinition Height="{Binding Block[Show_ChangeDir].Value, Source={StaticResource MasterData}, Converter={FlsBase:ObjectToBoolean true, {x:Static sys:Double.NaN}, 0, false}, FallbackValue=auto}"/>
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>
                                <GroupBox  Visibility="Visible" Header="{FlsBase:LangByCaption Fls.Language.AcesysV8::Grp.ExIn}" >
                                    <EccFace:EccItemsControl	FlsBase:Info.MimicPartTemplate="AcesysV8.FP_Template_LampLine"
