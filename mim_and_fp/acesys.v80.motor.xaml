                            <GroupBox Grid.IsSharedSizeScope="True" Header="{FlsBase:LangByCaption Fls.Language.AcesysV8::Grp.DisIn}" Grid.Row="4" Grid.ColumnSpan="2" >
                                <EccFace:EccItemsControl  FlsBase:Info.MimicPartTemplate="AcesysV8.FP_Template_AlarmsUsed"
														BlockParameters="{Binding Block, Source={StaticResource MasterData}}"
														CommonData="ACESYS_#Show_"
														BlockItemIndexPrefix="1"
														ItemsSpecification="RDY;MCC;OVL;LSTP;REM;SSW;ES;TFA;DRDY;STALL;EARTH;UBAL;GFA;GWA;BUSF;DOPR;DTEST;ACK;ASTR" >
