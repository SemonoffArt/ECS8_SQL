                                        <DiscreteInt32KeyFrame KeyTime="{Binding Value, Source={FlsBase:Retrieve V8FP_LoadTime, AcesysV8.SiteSpecific}, FallbackValue=0:0:1}" Value="1" Storyboard.TargetProperty="(FlsBase:Info.CommandPress)" />
                                    </Int32AnimationUsingKeyFrames>
                                </Storyboard>
                            </BeginStoryboard>
                        </EventTrigger.Actions>
                    </EventTrigger>
                </Control.Triggers>
            </Control>
            -->
            <Control Name="SHOW_AI1" IsEnabled="{Binding Block[SHOW_AI1].Value, Source={StaticResource MasterData}, FallbackValue=Fals}"      Width="0" Height="0" />
            <Control Name="SHOW_AI2" IsEnabled="{Binding Block[SHOW_AI2].Value, Source={StaticResource MasterData}, FallbackValue=False}"      Width="0" Height="0" />
            <Control Name="SHOW_AI3" IsEnabled="{Binding Block[SHOW_AI3].Value, Source={StaticResource MasterData}, FallbackValue=False}"      Width="0" Height="0" />
            <Control Name="SHOW_SP" IsEnabled="{Binding Block[SHOW_SP].Value, Source={StaticResource MasterData}, FallbackValue=False}"      Width="0" Height="0" />
            <Control Name="SHOW_DRDY" IsEnabled="{Binding Block[SHOW_DRDY].Value, Source={StaticResource MasterData}, FallbackValue=False}"    Width="0" Height="0" />
            <Control Name="SHOW_TFA" IsEnabled="{Binding Block[SHOW_TFA].Value, Source={StaticResource MasterData}, FallbackValue=False}"    Width="0" Height="0" />
            <Control Name="SHOW_STALL" IsEnabled="{Binding Block[SHOW_STALL].Value, Source={StaticResource MasterData}, FallbackValue=False}"    Width="0" Height="0" />
            <Control Name="SHOW_EARTH" IsEnabled="{Binding Block[SHOW_EARTH].Value, Source={StaticResource MasterData}, FallbackValue=False}"    Width="0" Height="0" />
            <Control Name="SHOW_UBAL" IsEnabled="{Binding Block[SHOW_UBAL].Value, Source={StaticResource MasterData}, FallbackValue=False}"    Width="0" Height="0" />
            <Control Name="SHOW_GFA" IsEnabled="{Binding Block[SHOW_GFA].Value, Source={StaticResource MasterData}, FallbackValue=False}"      Width="0" Height="0" />
            <Control Name="SHOW_GWA" IsEnabled="{Binding Block[SHOW_GWA].Value, Source={StaticResource MasterData}, FallbackValue=False}"      Width="0" Height="0" />
            <Control Name="SHOW_BUSF" IsEnabled="{Binding Block[SHOW_BUSF].Value, Source={StaticResource MasterData}, FallbackValue=False}"    Width="0" Height="0" />
            <Control Name="SHOW_DOPR" IsEnabled="{Binding Block[SHOW_DOPR].Value, Source={StaticResource MasterData}, FallbackValue=False}"  Width="0" Height="0" />
            <Control Name="SHOW_DTEST" IsEnabled="{Binding Block[SHOW_DTEST].Value, Source={StaticResource MasterData}, FallbackValue=False}"  Width="0" Height="0" />
            <Control Name="SHOW_ACK" IsEnabled="{Binding Block[SHOW_ACK].Value, Source={StaticResource MasterData}, FallbackValue=Fals}"      Width="0" Height="0" />
        </Grid>
    </Grid>

    <DataTemplate.Triggers>

        <MultiDataTrigger>
            <MultiDataTrigger.Conditions>
                <Condition Binding="{Binding IsEnabled, ElementName=Sel_INCH_Man}" Value="True" />
                <Condition Binding="{Binding IsEnabled, ElementName=MAN}" Value="True" />
            </MultiDataTrigger.Conditions>
            <Setter TargetName="CmdUnimotorInching" Property="Visibility" Value="Visible" />
            <Setter TargetName="CmdUnimotor" Property="Visibility" Value="Collapsed" />
            <Setter TargetName="CmdBimotorInching" Property="Visibility" Value="Visible" />
            <Setter TargetName="CmdBimotor" Property="Visibility" Value="Collapsed" />
        </MultiDataTrigger>


        <MultiDataTrigger>
            <MultiDataTrigger.Conditions>
                <Condition Binding="{Binding IsEnabled, ElementName=SHOW_AI1}" Value="False" />
                <Condition Binding="{Binding IsEnabled, ElementName=SHOW_AI2}" Value="False" />
                <Condition Binding="{Binding IsEnabled, ElementName=SHOW_AI3}" Value="False" />
            </MultiDataTrigger.Conditions>
            <Setter TargetName="Show_Analog_Tab" Property="Visibility" Value="Collapsed" />
        </MultiDataTrigger>

        <MultiDataTrigger>
            <MultiDataTrigger.Conditions>
                <Condition Binding="{Binding IsEnabled, ElementName=SHOW_AI1}" Value="False" />
                <Condition Binding="{Binding IsEnabled, ElementName=SHOW_AI2}" Value="False" />
                <Condition Binding="{Binding IsEnabled, ElementName=SHOW_AI3}" Value="False" />
                <Condition Binding="{Binding IsEnabled, ElementName=SHOW_SP}" Value="False" />
            </MultiDataTrigger.Conditions>
            <Setter TargetName="AnalogValues" Property="Height" Value="0" />
        </MultiDataTrigger>

        <MultiDataTrigger>
            <MultiDataTrigger.Conditions>
                <Condition Binding="{Binding IsEnabled, ElementName=SHOW_TFA}" Value="False" />
                <Condition Binding="{Binding IsEnabled, ElementName=SHOW_DRDY}" Value="False" />
                <Condition Binding="{Binding IsEnabled, ElementName=SHOW_STALL}" Value="False" />
                <Condition Binding="{Binding IsEnabled, ElementName=SHOW_EARTH}" Value="False" />
                <Condition Binding="{Binding IsEnabled, ElementName=SHOW_UBAL}" Value="False" />
                <Condition Binding="{Binding IsEnabled, ElementName=SHOW_GFA}" Value="False" />
                <Condition Binding="{Binding IsEnabled, ElementName=SHOW_GWA}" Value="False" />
                <Condition Binding="{Binding IsEnabled, ElementName=SHOW_BUSF}" Value="False" />
                <Condition Binding="{Binding IsEnabled, ElementName=SHOW_DOPR}" Value="False" />
                <Condition Binding="{Binding IsEnabled, ElementName=SHOW_DTEST}" Value="False" />
                <Condition Binding="{Binding IsEnabled, ElementName=SHOW_ACK}" Value="False" />
            </MultiDataTrigger.Conditions>
            <Setter TargetName="Ext_Input" Property="Height" Value="0" />
        </MultiDataTrigger>
        <!--
        <DataTrigger Binding="{Binding IsEnabled, ElementName=INTL}" Value="False">
            <Setter TargetName="TabCtrl" Property="SelectedIndex" Value="1" />
